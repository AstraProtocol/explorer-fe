{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"jsx": "preserve",
		"incremental": true,
		"baseUrl": ".",
		"types": ["cypress", "node", "./support"],
		"isolatedModules": false
	},
	"include": ["**/*.ts"],
	"exclude": []
}
